<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1"
         metadata-complete="true">

  <!--计算机-->
  <servlet>
    <servlet-name>calc</servlet-name>
    <servlet-class>MyCalc</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>calc</servlet-name>
    <url-pattern>/calc</url-pattern>
  </servlet-mapping>

  <!--刷新-->
  <servlet>
    <servlet-name>fresh</servlet-name>
    <servlet-class>MyFresh</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>fresh</servlet-name>
    <url-pattern>/fresh</url-pattern>
  </servlet-mapping>

  <!--跳转-->
  <servlet>
    <servlet-name>jump</servlet-name>
    <servlet-class>MyJumpServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>jump</servlet-name>
    <url-pattern>/jump</url-pattern>
  </servlet-mapping>

</web-app>
